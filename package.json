  "scripts": {
    "build:spa": "cross-env NODE_ENV=production webpack --mode production --config config/build.conf.js",
    "build:ssr": "babel src/ssr.js --out-file dist/ssr.js",
    "build": "yarn run build:spa && yarn run build:ssr",
    "unit": "cross-env BABEL_ENV=test NODE_ENV=testing karma start test/unit/karma.conf.js --watch",
    "test": "cross-env BABEL_ENV=test NODE_ENV=testing karma start test/unit/karma.conf.js --single-run",
    "test:types": "tsc -p ./types/tsconfig.json",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "lint:fix": "eslint --fix .js,.vue src test/unit/specs",
    "finish": "yarn run lint && yarn test && yarn run build",
    "publish": "git push && git push --tags && yarn publish"
  },
